alias zathura='env GDK_SCALE=1 GDK_DPI_SCALE=2.25 zathura'
alias spotify='spotify --force-device-scale-factor=2.25' 
alias chromium='chromium --force-device-scale-factor=2.25 --disk-cache-dir=/tmp/chromium_cache'
alias vim='/usr/bin/nvim'
alias matlab='env _JAVA_OPTIONS="" matlab -nodesktop -nosplash'
alias ls='ls --color=auto'
alias open='mimeopen'
alias sudo='sudo -E'
alias ecad='source /opt/ecad/setup.zsh'
alias ssh='ssh -Y'
